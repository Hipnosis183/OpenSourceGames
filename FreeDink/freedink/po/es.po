#: src/app.cpp:182
#, c-format
msgid "Report bugs to %s.\n"
msgstr ""
"Comunicar defectos a %s.\n"
"Comunique fallos de traducción a <translation-team-es@lists.sourceforge.net>\n"

#: src/app.cpp:312
#, fuzzy, c-format
msgid "Note: -nomovie is accepted for compatibility, but has no effect.\n"
msgstr "Nota: -nomovie es aceptado por compatibilidad pero no tiene ningún efecto.\n"

#: src/AppFreeDink.cpp:136
msgid "Starts the Dink Smallwood game or one of its D-Mods."
msgstr "Inicia el juego Dink Smallwood o uno de sus D-Mods."

#: src/brain_keyboard.cpp:284
msgid "`$I don't see anything here."
msgstr "`$No veo nada aquí."

#: src/brain_keyboard.cpp:285
msgid "`$Huh?"
msgstr "`$¿Huh?"

#: src/brain_keyboard.cpp:286
msgid "`$I'm fairly sure I can't talk to or use that."
msgstr "`$Te aseguro que no puedo hablar con eso ni usarlo."

#: src/brain_keyboard.cpp:287
msgid "`$What?"
msgstr "`$¿Qué?"

#: src/brain_keyboard.cpp:288
msgid "`$I'm bored."
msgstr "`$Me aburro."

#: src/brain_keyboard.cpp:289
msgid "`$Not much happening here."
msgstr "`$No pasa nada aquí."

#: src/brain_keyboard.cpp:409
msgid "`$I don't know any magic."
msgstr "`$No se de ningún hechizo."

#: src/brain_keyboard.cpp:410
msgid "`$I'm no wizard!"
msgstr "`$¡No soy un mago!"

#: src/brain_keyboard.cpp:411
msgid "`$I need to learn magic before trying this."
msgstr "`$Necesito aprender magia antes de intentar esto."

#: src/brain_keyboard.cpp:412
msgid "`$I'm gesturing wildly to no avail!"
msgstr "`$¡Estoy gesticulando como loco sin ningún sentido!"

#: src/brain_keyboard.cpp:413
msgid "`$Nothing happened."
msgstr "`$No pasó nada."

#: src/brain_keyboard.cpp:414
msgid "`$Hocus pocus!"
msgstr "`$¡Hocus pocus!"

