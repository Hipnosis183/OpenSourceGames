#: src/app.cpp:182
#, c-format
msgid "Report bugs to %s.\n"
msgstr ""
"Segnalare i problemi a %s.\n"
"Segnalare i problemi di traduzione a tp@lists.linux.it.\n"

#: src/app.cpp:312
#, fuzzy, c-format
msgid "Note: -nomovie is accepted for compatibility, but has no effect.\n"
msgstr "Nota: -nomovie viene accettato per motivi di compatibilità, ma non ha alcun effetto.\n"

#: src/AppFreeDink.cpp:136
msgid "Starts the Dink Smallwood game or one of its D-Mods."
msgstr "Avvia il gioco Dink Smallwood o uno dei suoi D-Mod."

#: src/brain_keyboard.cpp:284
msgid "`$I don't see anything here."
msgstr "`$Non vedo nulla qui."

#: src/brain_keyboard.cpp:285
msgid "`$Huh?"
msgstr "`$Come, prego?"

#: src/brain_keyboard.cpp:286
msgid "`$I'm fairly sure I can't talk to or use that."
msgstr "`$Sono abbastanza sicuro di non poterlo utilizzare e di non poterci parlare."

#: src/brain_keyboard.cpp:287
msgid "`$What?"
msgstr "`$Cosa?"

#: src/brain_keyboard.cpp:288
msgid "`$I'm bored."
msgstr "`$Mi sto annoiando."

#: src/brain_keyboard.cpp:289
msgid "`$Not much happening here."
msgstr "`$Non succede molto qui."

#: src/brain_keyboard.cpp:409
msgid "`$I don't know any magic."
msgstr "`$Non conosco alcuna magia."

#: src/brain_keyboard.cpp:410
msgid "`$I'm no wizard!"
msgstr "`$Non sono un mago."

#: src/brain_keyboard.cpp:411
msgid "`$I need to learn magic before trying this."
msgstr "`$Prima di provarci devo imparare la magia."

#: src/brain_keyboard.cpp:412
msgid "`$I'm gesturing wildly to no avail!"
msgstr "`$Sto facendo segni all'impazzata inutilmente."

#: src/brain_keyboard.cpp:413
msgid "`$Nothing happened."
msgstr "`$Non è successo nulla."

#: src/brain_keyboard.cpp:414
msgid "`$Hocus pocus!"
msgstr "`$Hocus pocus!"

